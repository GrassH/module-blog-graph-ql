        <arguments>
            <argument name="extendedConfigData">
                <item name="posts_per_page" xsi:type="string">mfblog/post_list/posts_per_page</item>
                <item name="sidebar_search" xsi:type="string">mfblog/sidebar/search/enabled</item>
                <item name="sidebar_categories" xsi:type="string">mfblog/sidebar/categories/enabled</item>
                <item name="sidebar_recent_posts" xsi:type="string">mfblog/sidebar/recent_posts/enabled</item>
                <item name="sidebar_tag" xsi:type="string">mfblog/sidebar/tag_claud/enabled</item>
